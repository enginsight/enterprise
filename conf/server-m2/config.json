{
    "database": {
      "uriConnectionString": "%%MONGODB_URI%%"
    },
    "api": {
      "port": 8080,
      "timeout": 5000,
      "cookies": {
        "maxAge": 3600000,
        "domain": "",
        "path": "/",
        "signed": false
      },
      "recaptcha": {
        "secret": "",
        "host": "www.google.com",
        "path": "/recaptcha/api/siteverify",
        "method": "POST"
      },
      "headers": {},
      "jwt": {
        "secret": "%%JWT_SECRET%%",
        "expiresIn": "24h"
      },
      "app": {
        "host": "%%APP_URL%%"
      },
      "master": {
        "organisation": "",
        "accessKey": {
          "id": "",
          "secret": ""
        }
      }
    },
    "cache": {
      "provider": "redis",
      "uriConnectionString": "%%REDIS_URI%%"
    },
    "email": {
      "host": "",
      "port": "",
      "sslTls": true,
      "user": "",
      "pass": "",
      "maxConnections": 2,
      "rateDelta": 1000,
      "rateLimit": 1
    }
  }
